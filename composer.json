{
    "name": "yoelpc4/laravel-stock",
    "description": "Laravel Stock service.",
    "keywords": [
        "laravel",
        "stock",
        "yahoo finance"
    ],
    "type": "library",
    "homepage": "https://github.com/yoelpc4/laravel-stock",
    "license": "MIT",
    "authors": [
        {
            "name": "Yoel Peter Chandra",
            "email": "yoelpc4@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/container": "8.*|7.*|6.*|5.8.*",
        "illuminate/contracts": "8.*|7.*|6.*|5.8.*",
        "illuminate/support": "8.*|7.*|6.*|5.8.*",
        "illuminate/validation": "8.*|7.*|6.*|5.8.*",
        "psr/http-client": "^1.0.1",
        "psr/http-message": "^1.0.1"
    },
    "require-dev": {
        "orchestra/testbench": "6.*|5.*|4.*|3.8.*",
        "phpunit/phpunit": "9.*|8.4.*|8.3.*|7.5.*"
    },
    "autoload": {
        "psr-4": {
            "Yoelpc4\\LaravelStock\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yoelpc4\\LaravelStock\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yoelpc4\\LaravelStock\\StockServiceProvider"
            ],
            "aliases": {
                "StockService": "Yoelpc4\\LaravelStock\\StockService"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
